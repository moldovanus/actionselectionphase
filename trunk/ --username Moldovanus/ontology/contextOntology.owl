<?xml version="1.0"?>
<rdf:RDF
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlx="http://swrl.stanford.edu/ontologies/built-ins/3.3/swrlx.owl#"
    xmlns:swrlm="http://swrl.stanford.edu/ontologies/built-ins/3.4/swrlm.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:query="http://swrl.stanford.edu/ontologies/built-ins/3.3/query.owl#"
    xmlns:temporal="http://swrl.stanford.edu/ontologies/built-ins/3.3/temporal.owl#"
    xmlns:tbox="http://swrl.stanford.edu/ontologies/built-ins/3.3/tbox.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://www.owl-ontologies.com/Ontology1230214892.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:abox="http://swrl.stanford.edu/ontologies/built-ins/3.3/abox.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
  xml:base="http://www.owl-ontologies.com/Ontology1230214892.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://swrl.stanford.edu/ontologies/3.3/swrla.owl"/>
    <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="resource">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="context-element"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="policy">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="associated-resource"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="sensor"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#context-element"/>
  </owl:Class>
  <owl:Class rdf:ID="TemperaturePolicy">
    <rdfs:subClassOf rdf:resource="#policy"/>
  </owl:Class>
  <owl:Class rdf:ID="actuator">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="physical-resource"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="actor-resource">
    <rdfs:subClassOf rdf:resource="#resource"/>
  </owl:Class>
  <owl:Class rdf:ID="actor">
    <rdfs:subClassOf rdf:resource="#context-element"/>
  </owl:Class>
  <owl:Class rdf:ID="movement-sensor">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#sensor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#sensor">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#physical-resource"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="temperature-sensor">
    <rdfs:subClassOf rdf:resource="#sensor"/>
  </owl:Class>
  <owl:Class rdf:about="#physical-resource">
    <rdfs:subClassOf rdf:resource="#resource"/>
  </owl:Class>
  <owl:Class rdf:ID="Policy">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="EvaluatePolicyP"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="humidity-sensor">
    <rdfs:subClassOf rdf:resource="#sensor"/>
  </owl:Class>
  <owl:Class rdf:ID="HumidityPolicy">
    <rdfs:subClassOf rdf:resource="#policy"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#associated-resource">
    <rdfs:domain rdf:resource="#policy"/>
  </owl:ObjectProperty>
  <rdf:Property rdf:about="http://swrl.stanford.edu/ontologies/built-ins/3.3/temporal.owl#hasGranularity">
    <rdfs:domain rdf:resource="http://swrl.stanford.edu/ontologies/built-ins/3.3/temporal.owl#ValidTime"/>
  </rdf:Property>
  <owl:DatatypeProperty rdf:ID="has-policy-string"/>
  <owl:DatatypeProperty rdf:ID="has-measuring-unit"/>
  <owl:DatatypeProperty rdf:ID="has-web-service-URI"/>
  <owl:DatatypeProperty rdf:about="#EvaluatePolicyP">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Policy"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="has-type-of-service"/>
  <owl:DatatypeProperty rdf:ID="has-position-Z"/>
  <owl:DatatypeProperty rdf:ID="has-value-of-service">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#sensor"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="has-position-Y"/>
  <owl:DatatypeProperty rdf:ID="has-influence-Radius"/>
  <owl:DatatypeProperty rdf:ID="has-position-X"/>
  <swrl:Imp rdf:ID="Rule-2">
    <swrl:body>
      <swrl:AtomList>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:argument1>
              <swrl:Variable rdf:ID="HumRef"/>
            </swrl:argument1>
            <swrl:classPredicate rdf:resource="#context-element"/>
          </swrl:ClassAtom>
        </rdf:first>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:rest>
              <swrl:AtomList>
                <rdf:rest>
                  <swrl:AtomList>
                    <rdf:first>
                      <swrl:BuiltinAtom>
                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                        <swrl:arguments>
                          <rdf:List>
                            <rdf:first>
                              <swrl:Variable rdf:ID="elemPropValue0"/>
                            </rdf:first>
                            <rdf:rest>
                              <rdf:List>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#float"
                                >0.5</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                              </rdf:List>
                            </rdf:rest>
                          </rdf:List>
                        </swrl:arguments>
                      </swrl:BuiltinAtom>
                    </rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </swrl:AtomList>
                </rdf:rest>
                <rdf:first>
                  <swrl:DatavaluedPropertyAtom>
                    <swrl:propertyPredicate rdf:resource="#has-value-of-service"/>
                    <swrl:argument2 rdf:resource="#elemPropValue0"/>
                    <swrl:argument1 rdf:resource="#HumRef"/>
                  </swrl:DatavaluedPropertyAtom>
                </rdf:first>
              </swrl:AtomList>
            </rdf:rest>
            <rdf:first>
              <swrl:ClassAtom>
                <swrl:argument1 rdf:resource="#HumRef"/>
                <swrl:classPredicate rdf:resource="#humidity-sensor"/>
              </swrl:ClassAtom>
            </rdf:first>
          </swrl:AtomList>
        </rdf:rest>
      </swrl:AtomList>
    </swrl:body>
    <swrl:head>
      <swrl:AtomList>
        <rdf:first>
          <swrl:DatavaluedPropertyAtom>
            <swrl:propertyPredicate rdf:resource="#EvaluatePolicyP"/>
            <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</swrl:argument2>
            <swrl:argument1>
              <HumidityPolicy rdf:ID="HumidityPolicyI">
                <associated-resource>
                  <humidity-sensor rdf:ID="HumiditySensorI">
                    <has-value-of-service rdf:datatype="http://www.w3.org/2001/XMLSchema#float"
                    >0.2</has-value-of-service>
                  </humidity-sensor>
                </associated-resource>
              </HumidityPolicy>
            </swrl:argument1>
          </swrl:DatavaluedPropertyAtom>
        </rdf:first>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
      </swrl:AtomList>
    </swrl:head>
  </swrl:Imp>
  <swrl:Imp rdf:ID="Rule-1">
    <swrl:body>
      <swrl:AtomList>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:rest>
              <swrl:AtomList>
                <rdf:rest>
                  <swrl:AtomList>
                    <rdf:first>
                      <swrl:BuiltinAtom>
                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#lessThan"/>
                        <swrl:arguments>
                          <rdf:List>
                            <rdf:first rdf:resource="#elemPropValue0"/>
                            <rdf:rest>
                              <rdf:List>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#float"
                                >0.5</rdf:first>
                              </rdf:List>
                            </rdf:rest>
                          </rdf:List>
                        </swrl:arguments>
                      </swrl:BuiltinAtom>
                    </rdf:first>
                    <rdf:rest>
                      <swrl:AtomList>
                        <rdf:first>
                          <swrl:BuiltinAtom>
                            <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                            <swrl:arguments>
                              <rdf:List>
                                <rdf:first rdf:resource="#elemPropValue0"/>
                                <rdf:rest>
                                  <rdf:List>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#float"
                                    >0.0</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                  </rdf:List>
                                </rdf:rest>
                              </rdf:List>
                            </swrl:arguments>
                          </swrl:BuiltinAtom>
                        </rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </swrl:AtomList>
                    </rdf:rest>
                  </swrl:AtomList>
                </rdf:rest>
                <rdf:first>
                  <swrl:DatavaluedPropertyAtom>
                    <swrl:propertyPredicate rdf:resource="#has-value-of-service"/>
                    <swrl:argument2 rdf:resource="#elemPropValue0"/>
                    <swrl:argument1>
                      <swrl:Variable rdf:ID="TempRef"/>
                    </swrl:argument1>
                  </swrl:DatavaluedPropertyAtom>
                </rdf:first>
              </swrl:AtomList>
            </rdf:rest>
            <rdf:first>
              <swrl:ClassAtom>
                <swrl:argument1 rdf:resource="#TempRef"/>
                <swrl:classPredicate rdf:resource="#temperature-sensor"/>
              </swrl:ClassAtom>
            </rdf:first>
          </swrl:AtomList>
        </rdf:rest>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:argument1 rdf:resource="#TempRef"/>
            <swrl:classPredicate rdf:resource="#context-element"/>
          </swrl:ClassAtom>
        </rdf:first>
      </swrl:AtomList>
    </swrl:body>
    <swrl:head>
      <swrl:AtomList>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        <rdf:first>
          <swrl:DatavaluedPropertyAtom>
            <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</swrl:argument2>
            <swrl:argument1>
              <TemperaturePolicy rdf:ID="TemperaturePolicyI">
                <associated-resource>
                  <temperature-sensor rdf:ID="TemperatureSensorI">
                    <has-value-of-service rdf:datatype="http://www.w3.org/2001/XMLSchema#float"
                    >0.7</has-value-of-service>
                  </temperature-sensor>
                </associated-resource>
              </TemperaturePolicy>
            </swrl:argument1>
            <swrl:propertyPredicate rdf:resource="#EvaluatePolicyP"/>
          </swrl:DatavaluedPropertyAtom>
        </rdf:first>
      </swrl:AtomList>
    </swrl:head>
  </swrl:Imp>
</rdf:RDF>

